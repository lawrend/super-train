.DS_Store
.env.local
.env.development.local
.env.test.local
.env.production.local
.env
/tmp
/tmp/cache
/tmp/development_secret.txt

./log
/log

npm-debug.log*
yarn-debug.log*
yarn-error.log*

# testing
/coverage

# production
/build

# dependencies
/node_modules
/.pnp
.pnp.js
./client/node_modules*
./client/node_modules/.cache
/client/node_modules


